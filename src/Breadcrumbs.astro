---
import type { Props as NavProps } from './NavigationList.astro'
import { type Collection, findBreadcrumbEntries } from './utils.js'
import NavigationList from './NavigationList.astro'

export type Props = Omit<NavProps, 'entries'>

const attrs = Astro.props

const entries = await findBreadcrumbEntries(attrs.collection || 'pages', Astro.url.pathname)
---

<NavigationList entries={entries} disableCurrent {...attrs} />
